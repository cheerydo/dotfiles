def_key "a"
  add_item_to_playlist

def_key "A"
  add_selected_items

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  volume_down

def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  volume_up

def_key "left"
  scroll_up_album

def_key "right"
  scroll_down_album

def_key "space"
  select_item
  scroll_down

def_key "space"
  toggle_lyrics_update_on_song_change

def_key "space"
  toggle_visualization_type
