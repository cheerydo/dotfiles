URxvt*scrollBar: 	false
URxvt*transparent: 	false
!URxvt*shading:      40
!URxvt*depth:		32

! terminal colors
*background: #1d1d1d
*foreground: #ffffff
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff
! --- end of terminal colors section ---

! fontconfigs and copy/paste with url launching
URxvt.colorUL:          #4682B4
! URxvt.font: 			xft:Inconsolata-zi4:size=10
! Urxvt*font:             -*-termsyn-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt.font:             -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
URxvt.perl-ext-common:  default,clipboard,url-select,keyboard-select
Urxvt.keysym.M-Escape: 		perl:keyboard-select:activate
Urxvt.keysym.M-s: 			perl:keyboard-select:search
Urxvt.keysym.M-u: 			perl:url-select:select_next
Urxvt.keysym.M-c: 			perl:clipboard:copy
Urxvt.keysym.M-v: 			perl:clipboard:paste
Urxvt.keysym.M-C-v: 		perl:clipboard:paste_escaped
Urxvt.url-select.autocopy: 	true
Urxvt.url-select.button:	1
Urxvt.url-select.underline: true
Urxvt.url-select.launcher:	/usr/bin/chromium

URxvt.iso14755:         false

Xft.autohint:	False
Xft.lcdfilter:	lcddefault
Xft.hintstyle:	hintslight
Xft.hinting: 	True
Xft.antialias:  True
Xft.rgba:		rgb
Xft.dpi:		96

dzen2.font: 	-*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
dzen2.foreground: rgb:ff/ff/ff
dzen2.background: rgb:00/00/00
