URxvt*scrollBar: 	false
URxvt*transparent: 	false

! terminal colors
*background: #303030
*foreground: #ffffff
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff
! --- end of terminal colors section ---

! fontconfigs and copy/paste with url launching
URxvt.colorUL:              #4682B4
! URxvt.font: 		    xft:Inconsolata-zi4:size=10
! Urxvt*font:               -*-termsyn-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt.font:                 -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.keysym.M-Escape: 	    perl:keyboard-select:activate
URxvt.keysym.M-s: 	    perl:keyboard-select:search
URxvt.keysym.M-u: 	    perl:url-select:select_next
URxvt.keysym.M-c: 	    perl:clipboard:copy
URxvt.keysym.M-v: 	    perl:clipboard:paste
URxvt.keysym.M-C-v: 	    perl:clipboard:paste_escaped
URxvt.url-select.autocopy:  true
URxvt.url-select.button:    1
URxvt.url-select.underline: true
URxvt.url-select.launcher:  /usr/bin/chromium

URxvt.iso14755:             0

Xft.autohint:	            0
Xft.lcdfilter:	            lcddefault
Xft.hintstyle:	            hintslight
Xft.hinting: 	            1
Xft.antialias:              1
Xft.rgba:	            rgb
Xft.dpi:	            96

dzen2.font: 	            -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
dzen2.foreground:           rgb:ff/ff/ff
dzen2.background:           rgb:00/00/00
