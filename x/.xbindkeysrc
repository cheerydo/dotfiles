"amixer -c 0 set Master 5+ && notify-send `amixer get Master -M | grep -oE "[[:digit:]]*%"`"
  XF86AudioRaiseVolume

"amixer -c 0 set Master 5- && notify-send `amixer get Master -M | grep -oE "[[:digit:]]*%"`"
  XF86AudioLowerVolume

"amixer set Master toggle && notify-send -u urgent Muted"
  XF86AudioMute

"thunar"
  XF86HomePage

"claws-mail"
  XF86Mail

"termite -e 'bc -q'"
  XF86Calculator

"systemctl suspend"
  XF86Sleep

"scrot -m -e 'mv $f ~/pictures/screens/'"
  Print

"termite -e ncmpcpp"
  Alt + m

"keepass --auto-type"
  Mod4 + Shift + v

"firefox"
   Alt + W

"dmenu_run"
  Mod4 + p

"passmenu"
  Mod4 + Shift + p
