#/etc/khal/khal.conf.sample
[calendars]
[[home]]
path = ~/.calendar/home
color = light blue

[[church]]
path = ~/.calendar/church
color = light green

[[jess]]
path = ~/.calendar/jess
color = yellow

[sqlite]
path = ~/.calendar/khal.db

[default]
default_calendar = home
default_command = calendar
show_all_days = false
highlight_event_days = true
print_new = event

[highlight_days]
color = light blue

[locale]
local_timezone = America/New_York
default_timezone = America/New_York
firstweekday = 6
timeformat = %H:%M
dateformat = %m.%d
longdateformat = %m.%d.%Y
datetimeformat = %A: %b.%d. %H:%M
longdatetimeformat = %A: %b.%m.%Y %H:%M

[view]
frame = color
