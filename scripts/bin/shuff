#!/usr/bin/env sh
if [ $(command -v shuf) ]; then
    shuf -n "$1"
elif [ $(command -v shuffle) ]; then
    shuffle -f /dev/stdin -p "$1"
else
    awk 'BEGIN{
        "od -tu4 -N4 -A n /dev/urandom" | getline
        srand(0+$0)
    }
    {print rand()"\t"$0}' | sort -n | cut -f 2 | head -n "$1"
fi
