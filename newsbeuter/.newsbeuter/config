# General settings
browser ilinks %u 
display-article-progress yes
goto-next-feed no
save-path ~/documents/newsbeuters/
show-keymap-hint yes

# macro
macro h toggle-show-read-feeds

# key-bindings
unbind-key j
unbind-key J
unbind-key k
unbind-key K
unbind-key l
bind-key j next
bind-key k prev
bind-key J next-feed
bind-key K prev-feed
bind-key h quit
bind-key l open
